{
  "env": {
    "es6": true,
    "node": true,
    "browser": true,
    "mocha": true,
    "jasmine": true
  },
  "extends": [
    "eslint:recommended"
  ],
  
  "plugins": [
    "babel",
    "import"
  ],
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module"
  }
}